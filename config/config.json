{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "azure-blockchain-workbench-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/azureBlockchainWorkbench/AzureBlockchainWorkbenchWebPart.js",
          "manifest": "./src/webparts/azureBlockchainWorkbench/AzureBlockchainWorkbenchWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "d3": "https://d3js.org/d3.v5.min.js"
  },
  "localizedResources": {
    "AzureBlockchainWorkbenchWebPartStrings": "lib/webparts/azureBlockchainWorkbench/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}
